passes=20
batch=60 
for place in US China
do 
    python Tester.py -batch $batch -p $passes -file Saved_Models/C16_ERA5_US_big_1_2020_09_08_05_24 -place $place
    python Tester.py -batch $batch -p $passes -file Saved_Models/C16_ERA5_US_big_2_2020_09_08_10_16 -place $place
    python Tester.py -batch $batch -p $passes -file Saved_Models/C16_ERA5_US_big_3_2020_09_08_14_35 -place $place
done
