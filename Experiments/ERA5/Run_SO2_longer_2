epochs=60
it=1500
n_test_US=5
n_test_China=5
n_val=100

for seed in 3 4 5
do
	python Experiment_ERA5.py -lr 1e-4 -epochs $epochs -it $it -track True -G SO2 -batch 5 -A irrep_big -n_val $n_val -n_test_US $n_test_US -n_test_China $n_test_China -l 3. -cov 4 -data big -seed $seed > Results/SO2/SO2_longer_${seed}.txt
done


