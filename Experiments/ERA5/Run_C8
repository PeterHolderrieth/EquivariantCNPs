epochs=3 #0
it=1 #500
n_test_US=1 # 5
n_test_China=1 #5
n_val=1 #00

for seed in 1 2 3 4 5
do 
	python Experiment_ERA5.py -lr 5e-4 -epochs $epochs -it $it -track True -G C8 -batch 5 -A regular_middle -n_val $n_val -n_test_US $n_test_US -n_test_China $n_test_China -l 3. -cov 4 -data big -seed $seed > Results/C8/C8_${seed}.txt
done


