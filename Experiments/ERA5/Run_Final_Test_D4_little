batch=60 
for place in US China
do 
    python Tester.py -batch $batch -p $passes -file Saved_Models/D4_little_ERA5_US_big_1_2020_09_08_04_29 -place $place
    python Tester.py -batch $batch -p $passes -file Saved_Models/D4_little_ERA5_US_big_2_2020_09_08_08_17 -place $place
    python Tester.py -batch $batch -p $passes -file Saved_Models/D4_little_ERA5_US_big_3_2020_09_08_12_10 -place $place
done
