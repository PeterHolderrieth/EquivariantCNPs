epochs=3 #0
it=1 #500
n_test_US=1 #20
n_test_China=1 #20
n_val=1 #00

for seed in 4 5
do 
	python Experiment_ERA5.py -lr 5e-4 -epochs $epochs -it $it -track True -G SO2 -batch 5 -A irrep_big -n_val $n_val -n_test_US $n_test_US -n_test_China $n_test_China -l 3. -cov 4 -data big -seed $seed > Results/SO2/SO2_${seed}.txt
done


