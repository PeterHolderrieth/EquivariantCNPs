    python Tester.py -batch $batch -p $passes -file Saved_Models/D8_ERA5_US_big_2_2020_09_08_09_10 -place $place
    python Tester.py -batch $batch -p $passes -file Saved_Models/D8_ERA5_US_big_3_2020_09_08_13_07 -place $place
done
