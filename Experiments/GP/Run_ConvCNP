#Get the number of iterations, epochs and tests needed!
epochs=30
it=1500
n_test=20
n_val=100
track=True

for seed in 1 2 3 4 5 
do 
    python Experiment_GP.py -lr 1e-4 -epochs 30 -it $it -track True -G CNN -A big -n_test $n_test -n_val $n_val -l 3. -cov 4 -batch 5  -seed $seed -data rbf > Results/ConvCNP/ConvCNP_rbf_${seed}.txt
done 

for seed in 1 2 3 4 5 
do 
    python Experiment_GP.py -lr 1e-4 -epochs 30 -it $it -track True -G CNN -A big -n_test $n_test -n_val $n_val -l 3. -cov 4 -batch 5  -seed $seed -data div_free > Results/ConvCNP/ConvCNP_div_free_${seed}.txt
done 

for seed in 1 2 3 4 5 
do 
    python Experiment_GP.py -lr 1e-4 -epochs 30 -it $it -track True -G CNN -A big -n_test $n_test -n_val $n_val -l 3. -cov 4 -batch 5  -seed $seed -data curl_free > Results/ConvCNP/ConvCNP_curl_free_${seed}.txt
done 