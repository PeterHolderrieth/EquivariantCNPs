#Train best ConvCNP model:
epochs=30
it=6000
python Experiment_GP.py -lr 1e-4 -epochs 30/ -A paper -it 6000 -track True -G CNP -n_eval 10000 -n_val 400 -l 3. -batch 60 -file Saved_Models/CNP_choice_first
python Experiment_GP.py -lr 1e-4 -epochs 30 -A double -it 6000 -track True -G CNP -n_eval 10000 -n_val 400 -l 3. -batch 60 -file Saved_Models/CNP_choice_second
python Experiment_GP.py -lr 1e-4 -epochs 30 -A small  -it 6000 -track True -G CNP -n_eval 10000 -n_val 400 -l 3. -batch 60 -file Saved_Models/CNP_choice_third
python Experiment_GP.py -lr 1e-4 -epochs 30 -A big -it 6000 -track True -G CNP -n_eval 10000 -n_val 400 -l 3. -batch 60 -file Saved_Models/CNP_choice_fourth 
python Experiment_GP.py -lr 1e-4 -epochs 30 -A thin -it 6000 -track True -G CNP -n_eval 10000 -n_val 400 -l 3. -batch 60 -file Saved_Models/CNP_choice_fifth 
