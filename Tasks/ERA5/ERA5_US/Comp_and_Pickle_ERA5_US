
#grib_get_data -p dataDate,dataTime,validityDate,validityTime,shortName ERA5_US_Central.grib
MIN=1980
MAX=2018

for (( c=$MIN; c<=$MAX; c++ ))
do  
    echo "Year: $c"
    #Create an unformatted CSV file:
    python ../3_Compress_Data_Table_ERA5.py Data/"$c"_ERA5_US.csv Data/"$c"_ERA5_US.pickle
    echo "Saved pickle file"

    #Delete the unprocessed CSV file:
    rm Data/"$c"_ERA5_US.csv
    
    echo "Removed CSV File"
done

echo "Process completed."
